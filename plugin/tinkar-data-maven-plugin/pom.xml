		<dependency>
			<groupId>dev.ikm.tinkar</groupId>
			<artifactId>reasoner-elk-snomed</artifactId>
		</dependency>
		
		<dependency>
			<groupId>dev.ikm.tinkar</groupId>
			<artifactId>reasoner-elk-owl</artifactId>
		</dependency>		

		<dependency>
